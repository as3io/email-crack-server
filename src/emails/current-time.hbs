<mjml>
  <mj-head>
    <mj-title>Current Time</mj-title>
    <mj-font name="Raleway" href="https://fonts.googleapis.com/css?family=Raleway" />
  </mj-head>
  <mj-body>
    <mj-section>
      <mj-column>
        <mj-text font-size="15px">The current server time is:</mj-text>
        <mj-text font-size="15px" color="blue">{{moment-format (now) "MMMM Do, YYYY @ HH:mm:ss.SSSZ"}}</mj-text>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>
